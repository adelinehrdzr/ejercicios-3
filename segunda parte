public class main {

public static void main(String[] args){
   Coche miCoche  = new Coche();
   miCoche.SumarPuerta();
   System.out.println(miCoche.puertas);

  }
    static class  Coche {

        public int puertas = 4;
        public void SumarPuerta(){
            this.puertas++;
        }

   }
}
